    "node": "14.17.6",
    "npm": "6.14.15"
  },
  "devDependencies": {
    "@types/node": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "axios": "^0.21.4",
    "chalk": "^4.1.2",
    "commander": "^8.2.0",
    "eslint": "^7.32.0",
